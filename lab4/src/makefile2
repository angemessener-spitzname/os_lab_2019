parallel_sum: parallel_sum.o utils.o libsum.a
	gcc -o parallel_sum parallel_sum.o utils.o -L. -lsum -lpthread

parallel_sum.o: parallel_sum.c
	gcc -c parallel_sum.c  

utils.o : utils.c
	gcc -c utils.c 

libsum.a: sum.o
	ar cr libsum.a sum.o

sum.o: sum.c 
	gcc -c sum.c 

clean:
	rm -f *.o *.a binary